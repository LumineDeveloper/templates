{
    "version": "2",
    "templates": [
      {
        "type": 1,
        "name": "mongo-replset",
        "title": "MongoDB ReplicaSet",
        "description": "Open-source document-oriented database",
        "categories": ["database"],
        "platform": "linux",
        "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/mongo.png",
        "image": "luminestudio/dev:mongo-replset70",
        "ports": ["27017/tcp"],
        "env": [
          {
            "name": "REPLICASETS",
            "label": "ReplicaSet",
            "description": "Please do not left empty for data security."
          },
          {
            "name": "MONGO_INITDB_ROOT_USERNAME",
            "label": "Username",
            "description": "Please do not left empty for data security."
          },
          {
            "name": "MONGO_INITDB_ROOT_PASSWORD",
            "label": "Password",
            "description": "Please do not left empty for data security."
          }
        ],
        "command": ["--replSet", "${REPLICASETS}", "--bind_ip_all", "--keyFile", "/keyFile"],
        "volumes": [
          {
            "container": "/data/db",
            "bind": "/stacks/mongodb-replset/data"
          },
          {
            "container": "/data/configdb",
            "bind": "/stacks/mongodb-replset/config"
          }
        ]
    },
      {
        "type": 1,
        "name": "postgresql",
        "title": "PostgreSQL",
        "description": "The most advanced open-source database",
        "categories": ["database"],
        "platform": "linux",
        "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/postgres.png",
        "image": "postgres:latest",
        "network": "private",
        "ports": ["5432/tcp"],
        "env": [
          {
            "name": "POSTGRES_USER",
            "label": "Username"
          },
          {
            "name": "POSTGRES_PASSWORD",
            "label": "Password"
          }
        ],
        "volumes": [
          {
            "container": "/var/lib/postgresql/data",
            "bind": "/stacks/postgresql/data"
          }
        ]
    }
  ]
}